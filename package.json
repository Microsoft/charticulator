{
  "name": "charticulator",
  "version": "1.1.0",
  "private": true,
  "author": {
    "name": "Donghao Ren",
    "email": "donghao.ren@gmail.com",
    "url": "https://donghaoren.org"
  },
  "contributors": [
    "David Tittsworth <datittsw@microsoft.com>"
  ],
  "scripts": {
    "precommit": "lint-staged",
    "dev": "node build.js",
    "build": "cross-env NODE_ENV=production node build.js",
    "server": "node build.js server",
    "public_server": "node build.js public_server",
    "watch": "node build.js watch",
    "start": "node build.js dev watch",
    "lint": "tslint 'src/**/*.{ts,tsx}'",
    "prettify": "prettier 'src/**/*.{ts,tsx}'",
    "unit_test": "NODE_PATH=. mocha dist/scripts/tests",
    "test": "yarn run lint && yarn run dev && yarn run unit_test",
    "add_licenses": "license-check-and-add"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "prettier --write",
      "license-check-and-add"
    ],
    "*.{js,jsx}": "license-check-and-add"
  },
  "dependencies": {
    "d3-dsv": "^1.0.8",
    "d3-force": "^1.1.0",
    "d3-format": "^1.3.0",
    "d3-time-format": "^2.1.1",
    "file-saver": "1.3.8",
    "hammerjs": "2.0.8",
    "lscg-solver": "1.0.1",
    "react": "16.3.2",
    "react-dom": "16.3.2"
  },
  "license-check-and-add-config": {
    "folder": ".",
    "license": "header.txt",
    "exact_paths_method": "INCLUDE",
    "exact_paths": [
      "src"
    ],
    "file_type_method": "INCLUDE",
    "file_types": [
      ".js",
      ".tsx",
      ".ts"
    ],
    "insert_license": true
  },
  "devDependencies": {
    "@types/chai": "4.0.1",
    "@types/d3": "4.9.0",
    "@types/file-saver": "1.3.0",
    "@types/hammerjs": "2.0.35",
    "@types/mocha": "2.2.41",
    "@types/node": "9.6.2",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "chai": "4.1.2",
    "cross-env": "^5.2.0",
    "file-loader": "^1.1.11",
    "fs-extra": "5.0.0",
    "http-server": "0.11.1",
    "husky": "^0.14.3",
    "js-yaml": "3.11.0",
    "license-check-and-add": "^2.3.6",
    "lint-staged": "^7.1.0",
    "mocha": "5.0.5",
    "multirun": "^3.0.2",
    "node-sass": "^4.9.0",
    "pegjs": "0.10.0",
    "preact": "^8.3.1",
    "preact-compat": "^3.18.3",
    "prettier": "^1.12.1",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.12.0",
    "tslint-react": "^3.6.0",
    "typescript": "2.8.1",
    "uglify-es": "3.3.9",
    "url-loader": "^1.0.1",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8"
  }
}
